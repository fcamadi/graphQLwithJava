type Query {
    countries: [Country]
}

type Country {
    name: String
    population: Int
    area: Int
    capital: City
    provinces: [Province]
}

type City {
    name: String
    population: Int
    geoLocation: GeoCoord
    elevation: Int
    province: Province
}

type Province {
    name: String
    population: Int
    capital: City
}

type GeoCoord {
    latitude: Float
    longitude: Float
}
